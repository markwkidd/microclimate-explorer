<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Microclimate_Explorer.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Microclimate_Explorer"
    Shell.FlyoutBehavior="Locked"
    Title="Microclimate Explorer">

    <FlyoutItem Title="Location">
        <ShellContent
            Title="Microclimate Explorer v0.1"
            ContentTemplate="{DataTemplate local:LocationPage}"
            Route="LocationPage" />
    </FlyoutItem>
</Shell>
